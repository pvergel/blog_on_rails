<h1> Blog on DeRails!</h1>

<h1>Listing posts</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.text %></td>
      <td><%= post.picture %></td>
    </tr>
  <% end %>
</table>




<%= link_to 'New post', new_post_path %>